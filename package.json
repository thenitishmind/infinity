{
	"name": "infinity-dev",
	"displayName": "Infinity",
	"description": "Infinite possibilities in coding - An autonomous AI coding agent right in your IDE, capable of creating/editing files, running commands, using the browser, and more with your permission every step of the way.",
	"version": "1.0.0",
	"icon": "assets/icons/infinity-icon.png",
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "Infinity Bot Inc."
	},
	"license": "Apache-2.0",
	"publisher": "saoudrizwan",
	"repository": {
		"type": "git",
		"url": "https://github.com/infinity/infinity"
	},
	"homepage": "https://infinity.bot",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"infinity",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"unlimited",
		"possibilities"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished",
		"workspaceContains:evals.env"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"walkthroughs": [
			{
				"id": "InfinityWalkthrough",
				"title": "Meet Infinity, your new coding partner",
				"description": "Infinity codes like a developer because it thinks like one. Here are 5 ways to put it to work:",
				"steps": [
					{
						"id": "welcome",
						"title": "Start with a Goal, Not Just a Prompt",
						"description": "Tell Infinity what you want to achieve. It plans, asks, and then codes, like a true partner.",
						"media": {
							"markdown": "walkthrough/step1.md"
						}
					},
					{
						"id": "learn",
						"title": "Let Infinity Learn Your Codebase",
						"description": "Point Infinity to your project. It builds understanding to make smart, context-aware changes.",
						"media": {
							"markdown": "walkthrough/step2.md"
						}
					},
					{
						"id": "advanced-features",
						"title": "Always Use the Best AI Models",
						"description": "Infinity empowers you with State-of-the-Art AI, connecting to top models (Anthropic, Gemini, OpenAI & more).",
						"media": {
							"markdown": "walkthrough/step3.md"
						}
					},
					{
						"id": "mcp",
						"title": "Extend with Powerful Tools (MCP)",
						"description": "Connect to databases, APIs, or discover new capabilities in the MCP Marketplace.",
						"media": {
							"markdown": "walkthrough/step4.md"
						}
					},
					{
						"id": "getting-started",
						"title": "You're Always in Control",
						"description": "Review Infinity's plans and diffs. Approve changes before they happen. No surprises.",
						"media": {
							"markdown": "walkthrough/step5.md"
						},
						"content": {
							"path": "walkthrough/step5.md"
						}
					}
				]
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "infinity-ActivityBar",
					"title": "Infinity (âŒ˜+')",
					"icon": "assets/icons/infinity-logo.svg",
					"when": "isMac"
				},
				{
					"id": "infinity-ActivityBar",
					"title": "Infinity (Ctrl+')",
					"icon": "assets/icons/infinity-logo.svg",
					"when": "!isMac"
				}
			]
		},
		"views": {
			"infinity-ActivityBar": [
				{
					"type": "webview",
					"id": "infinity.SidebarProvider",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "infinity.plusButtonClicked",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "infinity.mcpButtonClicked",
				"title": "MCP Servers",
				"icon": "$(server)"
			},
			{
				"command": "infinity.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "infinity.popoutButtonClicked",
				"title": "Open in Editor",
				"icon": "$(link-external)"
			},
			{
				"command": "infinity.accountButtonClicked",
				"title": "Account",
				"icon": "$(account)"
			},
			{
				"command": "infinity.settingsButtonClicked",
				"title": "Settings",
				"icon": "$(settings-gear)"
			},
			{
				"command": "infinity.openInNewTab",
				"title": "Open In New Tab",
				"category": "Infinity"
			},
			{
				"command": "infinity.addToChat",
				"title": "Add to Infinity",
				"category": "Infinity"
			},
			{
				"command": "infinity.addTerminalOutputToChat",
				"title": "Add to Infinity",
				"category": "Infinity"
			},
			{
				"command": "infinity.focusChatInput",
				"title": "Jump to Chat Input",
				"category": "Infinity"
			},
			{
				"command": "infinity.generateGitCommitMessage",
				"title": "Generate Commit Message with Infinity",
				"category": "Infinity",
				"icon": "$(robot)"
			},
			{
				"command": "infinity.explainCode",
				"title": "Explain with Infinity",
				"category": "Infinity"
			},
			{
				"command": "infinity.improveCode",
				"title": "Improve with Infinity",
				"category": "Infinity"
			},
			{
				"command": "infinity.openWalkthrough",
				"title": "Open Walkthrough",
				"category": "Infinity"
			}
		],
		"keybindings": [
			{
				"command": "infinity.addToChat",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "editorHasSelection"
			},
			{
				"command": "infinity.generateGitCommitMessage",
				"when": "scmProvider == git"
			},
			{
				"command": "infinity.focusChatInput",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "!editorHasSelection"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "infinity.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == infinity.SidebarProvider"
				},
				{
					"command": "infinity.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == infinity.SidebarProvider"
				},
				{
					"command": "infinity.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == infinity.SidebarProvider"
				},
				{
					"command": "infinity.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == infinity.SidebarProvider"
				},
				{
					"command": "infinity.accountButtonClicked",
					"group": "navigation@5",
					"when": "view == infinity.SidebarProvider"
				},
				{
					"command": "infinity.settingsButtonClicked",
					"group": "navigation@6",
					"when": "view == infinity.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "infinity.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == infinity.TabPanelProvider"
				},
				{
					"command": "infinity.mcpButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == infinity.TabPanelProvider"
				},
				{
					"command": "infinity.historyButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == infinity.TabPanelProvider"
				},
				{
					"command": "infinity.popoutButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == infinity.TabPanelProvider"
				},
				{
					"command": "infinity.accountButtonClicked",
					"group": "navigation@5",
					"when": "activeWebviewPanelId == infinity.TabPanelProvider"
				},
				{
					"command": "infinity.settingsButtonClicked",
					"group": "navigation@6",
					"when": "activeWebviewPanelId == infinity.TabPanelProvider"
				}
			],
			"editor/context": [
				{
					"command": "infinity.addToChat",
					"group": "navigation",
					"when": "editorHasSelection"
				}
			],
			"terminal/context": [
				{
					"command": "infinity.addTerminalOutputToChat",
					"group": "navigation"
				}
			],
			"scm/title": [
				{
					"command": "infinity.generateGitCommitMessage",
					"group": "navigation",
					"when": "scmProvider == git"
				}
			],
			"commandPalette": [
				{
					"command": "infinity.generateGitCommitMessage",
					"when": "scmProvider == git"
				}
			]
		},
		"configuration": {
			"title": "Infinity",
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"compile-standalone": "npm run check-types && npm run lint && node esbuild.js --standalone",
		"postcompile-standalone": "node scripts/package-standalone.mjs",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run check-types && npm run build:webview && npm run lint && node esbuild.js --production",
		"protos": "node scripts/build-proto.mjs && node scripts/generate-server-setup.mjs && node scripts/generate-host-bridge-client.mjs",
		"postprotos": "prettier src/shared/proto src/core/controller src/hosts/ webview-ui/src/services src/generated --write --log-level warn",
		"clean": "rimraf dist dist-standalone webview-ui/build src/generated out/",
		"check-types": "npm run protos && npx tsc --noEmit && cd webview-ui && npx tsc -b --noEmit",
		"lint": "eslint src --ext ts && eslint webview-ui/src --ext ts && buf lint && cd webview-ui && npm run lint",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"install:all": "npm install && cd webview-ui && npm install",
		"dev": "npm-run-all -p watch:* dev:webview",
		"dev:webview": "cd webview-ui && npm run dev",
		"build:webview": "cd webview-ui && npm run build",
		"publish:marketplace": "vsce publish && ovsx publish",
		"publish:marketplace:prerelease": "vsce publish --pre-release && ovsx publish --pre-release",
		"prepare": "husky",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"docs": "cd docs && npm run dev",
		"docs:check-links": "cd docs && npm run check",
		"docs:rename-file": "cd docs && npm run rename",
		"report-issue": "node scripts/report-issue.js"
	},
	"lint-staged": {
		"*": [
			"prettier --write --ignore-unknown --log-level=log"
		]
	},
